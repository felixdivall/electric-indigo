			"editorLineNumber.foreground": "#7870ab", // Color of editor line numbers.
			"editorCursor.foreground": "#FFD700", //  Color of the editor cursor.

			// The selection color battle starts here.
			"editor.selectionBackground": "#5706a288", // Color of the editor selection.
