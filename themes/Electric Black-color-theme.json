{
	// Theme Color reference.
	// https://code.visualstudio.com/docs/getstarted/theme-color-reference
	"name": "Electric Black",
		"$schema": "vscode://schemas/color-theme",
		"type": "dark",
		"semanticHighlighting": true,

		"semanticTokenColors": {
			"enum": {
				"foreground": "#D3FFA3"
		  	},
			"enumMember": {
				"foreground": "#b8fb71"
			},
			"property": {
				"foreground": "#FFD700"
			},
			"class.defaultLibrary": {
				"foreground": "#70fbcd"
				// #fe9b70 / #62ffcb / #86ffd7
			},
			"property.declaration": {
				"foreground": "#FCEE95"
			},
			"namespace": {
				"foreground": "#FCEE95"
			},
			"interface": {
				"foreground": "#FFD700"
			},
			"interface.defaultLibrary": {
				"foreground": "#70fbcd"
			},
			"*.async": {
				"foreground": "#fe9b70"
			},
			"variable": {
				"foreground": "#AAF5FF"
			},
			// "variable.constant": {
			// 	"foreground": "#8366d1"
			// },
			"variable.defaultLibrary": {
				"foreground": "#E1EFFF"
			},
